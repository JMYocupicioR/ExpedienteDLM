# ========================================
# CONFIGURACIÓN DE SUPABASE
# ========================================
# Obtén estas credenciales desde: https://supabase.com/dashboard/project/qcelbrzjrmjxpjxllyhk/settings/api
VITE_SUPABASE_URL=https://qcelbrzjrmjxpjxllyhk.supabase.co
VITE_SUPABASE_ANON_KEY=tu_supabase_anon_key_aqui

# Clave de servicio para Edge Functions (obtén desde: https://supabase.com/dashboard/project/qcelbrzjrmjxpjxllyhk/settings/api)
SUPABASE_SERVICE_ROLE_KEY=tu_supabase_service_role_key_aqui

# ========================================
# CONFIGURACIÓN DE HCAPTCHA
# ========================================
# Obtén estas credenciales desde: https://dashboard.hcaptcha.com/sites
VITE_HCAPTCHA_SITE_KEY=tu_hcaptcha_site_key_aqui
HCAPTCHA_SECRET_KEY=tu_hcaptcha_secret_key_aqui

# ========================================
# CONFIGURACIÓN DE OAUTH - GOOGLE
# ========================================
# Obtén estas credenciales desde: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=tu_google_client_id_aqui
GOOGLE_CLIENT_SECRET=tu_google_client_secret_aqui

# ========================================
# CONFIGURACIÓN DE OAUTH - FACEBOOK
# ========================================
# Obtén estas credenciales desde: https://developers.facebook.com/apps/
FACEBOOK_CLIENT_ID=tu_facebook_client_id_aqui
FACEBOOK_CLIENT_SECRET=tu_facebook_client_secret_aqui

# ========================================
# CONFIGURACIÓN DE SMTP
# ========================================
# Configuración para envío de emails
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=tu_email@gmail.com
SMTP_PASSWORD=tu_app_password_aqui
SMTP_FROM=tu_email@gmail.com

# ========================================
# CONFIGURACIÓN DE DEEPSEEK (OPCIONAL)
# ========================================
# Si usas DeepSeek para análisis de expedientes
VITE_DEEPSEEK_API_KEY=tu_deepseek_api_key_aqui

# ========================================
# CONFIGURACIÓN DE DESARROLLO
# ========================================
NODE_ENV=development
VITE_APP_ENV=development
